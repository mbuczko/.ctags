--regex-Rust=/fn +([a-zA-Z0-9_]+) *[(<{]/\1/f/
--regex-Rust=/(type|enum|struct|trait)[ \t]+([a-zA-Z0-9_]+) *[<{(;]/\2/T/
--regex-Rust=/mod[ \t]+([a-zA-Z0-9_]+) *[<{(;]/\1/M/
--regex-Rust=/(static|const) +([a-zA-Z0-9_]+) *[:=]/\2/c/
--regex-Rust=/macro_rules! +([a-zA-Z0-9_]+) *[{]/\1/d/
--regex-Rust=/impl([ \t\n]*<[^>]*>)?[ \t]+(([a-zA-Z0-9_:]+)[ \t]*(<[^>]*>)?[ \t]+(for)[ \t]+)?([a-zA-Z0-9_]+)/\6/i/
